<?xml version="1.0"?>
<robot name="dbw_mkz" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:arg name="dae_file" default="$(find dbw_mkz_description)/meshes/body.dae"/>
  
    <link name="base_footprint">
    </link>

    <link name="base_link">
      <inertial>
        <origin xyz="0 0 0.071011" rpy="0 0 0"/>
        <mass value="1500.0"/>
        <inertia ixx="1000.0" ixy="0" ixz="0" iyy="1000.0" iyz="0" izz="1000.0"/>
      </inertial>
      <visual name="Raw Windshield">
        <origin xyz="0.667205 0 0.859831" rpy="0 0 1.570796"/>
        <geometry>
          <mesh filename="package://dbw_mkz_description/meshes/windshield.dae" scale="1.0 1.0 1.0"/>
        </geometry>
      </visual>
      <visual name="Raw_Back_Window">
        <origin xyz="-1.466892 0 0.926" rpy="0 0 -1.570796"/>
        <geometry>
          <mesh filename="package://dbw_mkz_description/meshes/back_window.dae" scale="1.0 1.0 1.0"/>
        </geometry>
        <material name="Material_001.001"/>
      </visual>
      <visual name="chassis">
        <origin xyz="-0.04 0 -0.33" rpy="0 0 1.570796"/>
        <geometry>
          <mesh filename="file://$(arg dae_file)" scale="0.01 0.01 0.01"/>
        </geometry>
        <material name="roof"/>
      </visual>
      <collision name="chassis_collision">
        <origin xyz="0.084967 -0.03046 0.350609" rpy="1.570796 0 -3.141593"/>
        <geometry>
          <mesh filename="package://dbw_mkz_description/meshes/mkz_collision.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>
    </link>

    <link name="left_steer">
      <inertial>
        <origin xyz="-0.013054 -0.0295 0" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.037227" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.06"/>
      </inertial>
    </link>

    <link name="right_steer">
      <inertial>
        <origin xyz="-0.01305 0.0275 0" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.037227" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.06"/>
      </inertial>
    </link>

    <link name="wheel_fl">
      <inertial>
        <origin xyz="0 0 0" rpy="0 -3.141593 0"/>
        <mass value="10.0"/>
        <inertia ixx="0.305583" ixy="0" ixz="0" iyy="0.305583" iyz="0" izz="0.5445"/>
      </inertial>
      <visual name="wheel_fl_visual">
        <origin xyz="0.0004 -0.005 0.0775" rpy="-1.570796 -1.570796 0"/>
        <geometry>
          <mesh filename="package://dbw_mkz_description/meshes/wheel.dae" scale="0.01 0.01 0.01"/>
        </geometry>
        <material name="tire_001.001"/>
      </visual>
      <collision name="wheel_fl_collision">
        <origin xyz="0 0 0" rpy="0 -3.141593 0"/>
        <geometry>
          <cylinder radius="0.33" length="0.2"/>
        </geometry>
      </collision>
    </link>

    <link name="wheel_fr">
      <inertial>
        <origin xyz="0 0 0" rpy="0 3.141593 0"/>
        <mass value="10.0"/>
        <inertia ixx="0.305583" ixy="0" ixz="0" iyy="0.305583" iyz="0" izz="0.5445"/>
      </inertial>
      <visual name="wheel_fr_visual">
        <origin xyz="0.0004 -0.005 -0.0775" rpy="-1.570796 1.570796 0"/>
        <geometry>
          <mesh filename="package://dbw_mkz_description/meshes/wheel.dae" scale="0.01 0.01 0.01"/>
        </geometry>
        <material name="tire_001.002"/>
      </visual>
      <collision name="wheel_fr_collision">
        <origin xyz="0 0 0" rpy="0 3.141593 0"/>
        <geometry>
          <cylinder radius="0.33" length="0.2"/>
        </geometry>
      </collision>
    </link>

    <link name="wheel_rl">
      <inertial>
        <origin xyz="0 0 -0.021117" rpy="0 3.141593 0"/>
        <mass value="10.0"/>
        <inertia ixx="0.305583" ixy="0" ixz="0" iyy="0.305583" iyz="0" izz="0.5445"/>
      </inertial>
      <visual name="wheel_rl_visual">
        <origin xyz="-0.0004 -0.005 0.056382" rpy="-1.570796 -1.570796 0"/>
        <geometry>
          <mesh filename="package://dbw_mkz_description/meshes/wheel.dae" scale="0.01 0.01 0.01"/>
        </geometry>
        <material name="tire_001"/>
      </visual>
      <collision name="wheel_rl_collision">
        <origin xyz="0 0 -0.021118" rpy="0 3.141593 0"/>
        <geometry>
          <cylinder radius="0.33" length="0.2"/>
        </geometry>
      </collision>
    </link>

    <link name="wheel_rr">
      <inertial>
        <origin xyz="0 0 0.0205" rpy="0 3.141593 0"/>
        <mass value="10.0"/>
        <inertia ixx="0.305583" ixy="0" ixz="0" iyy="0.305583" iyz="0" izz="0.5445"/>
      </inertial>
      <visual name="wheel_rr_visual">
        <origin xyz="-0.0004 -0.005 -0.057" rpy="-1.570796 1.570796 0"/>
        <geometry>
          <mesh filename="package://dbw_mkz_description/meshes/wheel.dae" scale="0.01 0.01 0.01"/>
        </geometry>
        <material name="tire_001.003"/>
      </visual>
      <collision name="wheel_rr_collision">
        <origin xyz="0 0 0.0205" rpy="0 3.141593 0"/>
        <geometry>
          <cylinder radius="0.33" length="0.2"/>
        </geometry>
      </collision>
    </link>

    <joint name="base_link" type="fixed">
      <origin xyz="0 0 0.33" rpy="0 0 0"/>
      <parent link="base_footprint"/>
      <child link="base_link"/>
    </joint>

    <joint name="left_steer" type="revolute">
      <origin xyz="1.435054 0.817 0.33" rpy="0 0 0"/>
      <parent link="base_link"/>
      <child link="left_steer"/>
      <axis xyz="0 0 1.0"/>
      <limit lower="-0.6" upper="0.6" effort="0" velocity="0"/>
    </joint>

    <joint name="right_steer" type="revolute">
      <origin xyz="1.43505 -0.815 0.33" rpy="0 0 0"/>
      <parent link="base_link"/>
      <child link="right_steer"/>
      <axis xyz="0 0 1.0"/>
      <limit lower="-0.6" upper="0.6" effort="0" velocity="0"/>
    </joint>

    <joint name="wheel_fl" type="continuous">
      <origin xyz="-0.012654 -0.0295 -0.33" rpy="1.570796 0 -3.141593"/>
      <parent link="left_steer"/>
      <child link="wheel_fl"/>
      <axis xyz="0 0 1.0"/>
      <limit effort="0" velocity="22.0"/>
    </joint>

    <joint name="wheel_fr" type="continuous">
      <origin xyz="-0.01265 0.0275 -0.33" rpy="1.570796 0 -3.141593"/>
      <parent link="right_steer"/>
      <child link="wheel_fr"/>
      <axis xyz="0 0 1.0"/>
      <limit effort="0" velocity="22.0"/>
    </joint>

    <joint name="wheel_rl" type="continuous">
      <origin xyz="-1.4224 0.808618 0" rpy="1.570796 0 -3.141593"/>
      <parent link="base_link"/>
      <child link="wheel_rl"/>
      <axis xyz="0 0 1.0"/>
      <limit effort="5000.0" velocity="22.0"/>
    </joint>

    <joint name="wheel_rr" type="continuous">
      <origin xyz="-1.4224 -0.808 0" rpy="1.570796 0 -3.141593"/>
      <parent link="base_link"/>
      <child link="wheel_rr"/>
      <axis xyz="0 0 1.0"/>
      <limit effort="5000.0" velocity="22.0"/>
    </joint>

    <material name="Material_001.001">
      <color rgba="0.64 0.64 0.64 1.0"/>
    </material>

    <material name="roof">
      <color rgba="0.48 0.48 0.48 1.0"/>
    </material>

    <material name="tire_001">
      <color rgba="0.084706 0.084706 0.084706 1.0"/>
    </material>

    <material name="tire_001.001">
      <color rgba="0.084706 0.084706 0.084706 1.0"/>
    </material>

    <material name="tire_001.002">
      <color rgba="0.084706 0.084706 0.084706 1.0"/>
    </material>

    <material name="tire_001.003">
      <color rgba="0.084706 0.084706 0.084706 1.0"/>
    </material>

</robot>
